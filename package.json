{
  "name": "@shammy911/guard-sdk",
  "version": "1.0.0",
  "description": "Guard API client SDK for rate limiting & abuse protection",
  "keywords": [
    "guard",
    "rate-limit",
    "security",
    "abuse",
    "ddos",
    "sdk"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shammy911/guard-sdk.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/shammy911/guard-sdk/issues"
  },
  "homepage": "https://github.com/shammy911/guard-sdk#readme",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
